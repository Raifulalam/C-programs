//2.write a program to find the area of the triangle if base and hight are given//
#include<stdio.h>
int main()
{
int base,hight,area;
printf("Enter base of triangle ");
scanf("%d",&base);
printf("Enter the hight of triangle");
scanf("%d",&hight);
area=(base*hight)/2;
printf("Area of triangle is %d\n",area);
return 0;
}
