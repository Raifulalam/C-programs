/*write a program to find the maximum b/w three number using conditional operator
 and ternary operator*/
 #include<stdio.h>
 int main(){
    int a,b,c,max;
    printf("Enter three number to find the maximum");
    scanf("%d%d%d",&a,&b,&c);
   max=a>b?(a>c?a:c):(b>c?b:c);
   printf("The maximum number b/w %d, %d &%d is %d\n",a,b,c,max);
   return 0;
 }
