// write a program for day of week using switch case//
#include<stdio.h>
int main(){
    int week;
    printf("Enter day number(1-7):");
    scanf("%d",&week);
    switch (week)
    {
    case 1:
    printf("Monday");
    break;
    case 2:
    printf("Tuesday");
    break;
    
    case 3:
    printf("Wednesday");
    break;
    
    case 4:
    printf("Thursday");
    break;
    
    case 5:
    printf("Friday");
    break;
    
    case 6:
    printf("Sunday");
    break;
    case 7:
    printf("Saturday");
    break;
    default:
    printf("Invalid input! please enter week number between 1 to 7");
        break;
    }
}
